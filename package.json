    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "ace-builds": "^1.4.5",
    "babel-eslint": "^10.0.2",
    "brace": "^0.11.1",
    "clipboard": "^2.0.4",
