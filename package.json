{
  "name": "shine",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:modern": "vue-cli-service build --modern",
    "dev": "node --max_old_space_size=8000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --open",
    "gen": "node generate.js",
    "lint:style": "vue-cli-service lint:style",
    "postbuild": "node combine",
    "prebuild": "NODE_ENV=production node schematic",
    "prebuild:dev": "node schematic",
    "predev": "NODE_ENV=dev node schematic",
    "prestart": "nvm use 10"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@vue/compat": "^3.2.36",
    "@vue/web-component-wrapper": "^1.3.0",
    "jsbarcode": "^3.11.5",
    "lodash.clonedeep": "^4.5.0",
    "masonry-layout": "^4.2.2",
    "core-js": "^3.8.3",
    "vue": "^3.2.13"
  },
  "devDependencies": {
    "@samhammer/vue-cli-plugin-stylelint": "^2.1.0",
    "@types/ace": "0.0.48",
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.4",
    "@vue/cli-service": "~5.0.4",
    "@vue/compiler-sfc": "^3.2.36",
    "@vue/eslint-config-prettier": "^7.0.0",
    "ace-builds": "^1.5.3",
    "brace": "^0.11.1",
    "chalk": "^4.0.1",
    "clipboard": "^2.0.11",
    "concat": "^1.0.3",
    "dateformat": "^3.0.3",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "ejs-compiled-loader": "^3.1.0",
    "ejs-loader": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "fs-extra": "^10.1.0",
    "fullname": "^4.0.1",
    "git-branch": "^2.0.1",
    "glob": "^8.0.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.6.2",
    "pretty": "^2.0.0",
    "prompt": "^1.3.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-webpack-plugin": "^3.3.0",
    "universal-analytics": "^0.5.3",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
