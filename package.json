{
  "name": "shine",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:modern": "vue-cli-service build --modern",
    "dev": "node --max_old_space_size=8000 node_modules/@vue/cli-service/bin/vue-cli-service.js serve --open",
    "gen": "node generate.js",
    "lint:style": "vue-cli-service lint:style",
    "postbuild": "node combine",
    "prestart": "nvm use 10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "core-js": "^3.3.2",
    "document-register-element": "^1.13.2",
    "vue": "^2.6.10",
    "vue-custom-element": "^3.2.7",
    "vue2-hammer": "^2.1.2"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@types/ace": "0.0.42",
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "ace-builds": "^1.4.5",
    "babel-eslint": "^10.0.2",
    "babel-polyfill": "^6.26.0",
    "brace": "^0.11.1",
    "clipboard": "^2.0.4",
    "concat": "^1.0.3",
    "dateformat": "^3.0.3",
    "dotenv": "^8.1.0",
    "ejs-compiled-loader": "^1.1.0",
    "ejs-loader": "^0.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.2.2",
    "fs-extra": "^8.1.0",
    "fullname": "^4.0.1",
    "git-branch": "^2.0.1",
    "glob": "^7.1.4",
    "node-sass": "^4.13.0",
    "pretty": "^2.0.0",
    "prompt": "^1.0.0",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.23.1",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^1.0.4",
    "vue-template-compiler": "^2.6.10",
    "wrapper-webpack-plugin": "^2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie 11"
  ]
}
