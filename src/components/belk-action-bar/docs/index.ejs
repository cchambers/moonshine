  <section id="examples">
    <h2>Examples</h2>
    <h3>Default</h3>
    <lib-toolbar>
      <belk-action-bar></belk-action-bar>
      <sh-button variant="tertiary" click-event="demo-view-one">VIEW ONE</sh-button>
      <sh-button variant="tertiary" click-event="demo-view-two">VIEW TWO</sh-button>
      <sh-button variant="tertiary" click-event="demo-view-three">VIEW THREE</sh-button>
    </lib-toolbar>
    <script>
      document.addEventListener('demo-view-one', () => {
        const obj = {
          state: 'view1',
          data: {
            text: 'View One Text'
          }
        }
        sh.emit('action-bar-view', obj);
      });

      document.addEventListener('demo-view-two', () => {
        const obj = {
          state: 'view2',
          data: {
            text: 'Some other text'
          }
        }
        sh.emit('action-bar-view', obj);
      });

      document.addEventListener('demo-view-three', () => {
        const obj = {
          state: 'view3',
          data: {
            text: 'View Three Text'
          }
        }
        sh.emit('action-bar-view', obj);
      });
    </script>
  </section>

