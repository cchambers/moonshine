    <h3 id="events">Events</h3>

    <table class="events">
      <thead>
        <tr>
          <th>Event</th>
          <th>Payload Type</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>event-name</td>
          <td>
            Type
          </td>
          <td>
            Fires when something happens.
          </td>
        </tr>
      </tbody>
    </table>
  </section>

  <section id="examples">
    <h2>Examples</h2>
    <h3>Using <code>[event-only]</code> Prevents Navigation</h3>
    <lib-toolbar>
      <sh-dropnav event-only>
        <ul>
          <li>
            <a href="#" data-value="t">Top</a>
          </li>
          <li>
            <a href="#" data-value="r">Right</a>
          </li>
          <li>
            <a href="#" data-value="b">Bottom</a>
          </li>
          <li>
            <a href="#" data-value="l">Left</a>
          </li>
        </ul>
      </sh-dropnav>
    </lib-toolbar>

    <h3>Using <code>[target]</code> Opens in Another Window</h3>
    <lib-toolbar>
      <sh-dropnav target="_new">
        <ul>
          <li>
            <a href="https://google.com">Google</a>
          </li>
          <li>
            <a href="https://apple.com">Apple</a>
          </li>
          <li>
            <a href="https://netflix.com">Netflix</a>
          </li>
        </ul>
      </sh-dropnav>
    </lib-toolbar>
  </section>

  <section id="changelog">
    <h2>Changelog</h2>
    <dl>
      <dt>1.0.0</dt>
      <dd>Dropnav was created.</dd>
    </dl>
  </section>
</div>

<%- htmlWebpackPlugin.options.docsFoot %>
