<%- htmlWebpackPlugin.options.docsHead %>

<title><%= htmlWebpackPlugin.options.title %></title>

<div class="documentation contain">
  <h1>Dropnav</h1>
  <section id="usage">
    <h2>Usage</h2>
    <pre>
      <code class="language-html">
          &lt;sh-dropnav&gt;
            &lt;ul&gt;
              &lt;li&gt;
                &lt;a href="https://google.com"&gt;Google&lt;/a&gt;
              &lt;/li&gt;
              &lt;li&gt;
                &lt;a href="https://apple.com"&gt;Apple&lt;/a&gt;
              &lt;/li&gt;
              &lt;li&gt;
                &lt;a href="https://netflix.com"&gt;Netflix&lt;/a&gt;
              &lt;/li&gt;
            &lt;/ul&gt;
          &lt;/sh-dropnav&gt;
      </code>
    </pre>

  </section>

  <section id="reference" class="baseline30">
    <h2>Reference</h2>
    <h3 id="attributes">Attributes</h3>

    <table class="attributes">
      <thead>
        <tr>
          <th>Attribute</th>
          <th>Type</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>event-only</td>
          <td>Boolean</td>
          <td>Prevent default navigation and instead emit an event that can be handled elsewhere.</td>
        </tr>
        <tr>
          <td>target</td>
          <td>String</td>
          <td>Opens links in a new tab.</td>
        </tr>
      </tbody>
    </table>

    <h3 id="slots">Slots</h3>

    <table class="slots">
      <thead>
        <tr>
          <th>Slot</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>default</td>
          <td>
            Unordered list of links.
          </td>
        </tr>
      </tbody>
    </table>

    <!-- <h3 id="methods">Public Methods</h3> -->

