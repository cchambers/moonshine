  </section>

  <section id="examples">
    <h2>Examples</h2>
    <h3>Default</h3>

    <sh-button class="nav-trigger" click-event="show-mobile-nav">
      <belk-icon width="30" name="menu"></belk-icon>
    </sh-button>
      <belk-mobile-nav active>
        <div slot="primary">
          <ul>
            <li>
              <sh-accordion>
                  <div slot="header">
                    First thing
                  </div>
                  <div slot="body">
                  <sh-accordion>
                      <div slot="header">
                        <a href="https://google.com">INCEPTION</a>
                      </div>
                      <div slot="body">
                        Lorem ipsum dolor sit amet consectetur adipisicing elit.
                      </div>
                    </sh-accordion>
                  </div>
                </sh-accordion>
            </li>

            <li>
                <sh-accordion>
                    <div slot="header">
                      Second & Thing
                    </div>
                    <div slot="body">
                    <sh-accordion>
                        <div slot="header">
                          Inside Second & Things 
                        </div>
                        <div slot="body">
                          Lorem ipsum dolor sit amet consectetur adipisicing elit.
                        </div>
                      </sh-accordion>
                    </div>
                  </sh-accordion>
              </li>

          </ul>
        </div>
      </belk-mobile-nav>
  </section>

  <section id="changelog">
    <h2>Changelog</h2>
    <dl>
      <dt>0.1.0</dt>
      <dd>Belk Mobile Nav is under development.</dd>
    </dl>
  </section>

