    </table>

    <h3 id="events">Events</h3>

    <table class="events">
      <thead>
        <tr>
          <th>Event</th>
          <th>Payload Type</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>event-name</td>
          <td>
            Type
          </td>
          <td>
            Fires when something happens.
          </td>
        </tr>
      </tbody>
    </table>
  </section>

  <section id="examples">
    <h2>Examples</h2>
    <h3>Default</h3>
    <lib-toolbar>

      <sh-tabs collapse unique-id="example" for="demo-swapper">
        <sh-button default-value="first-view">First</sh-button>
        <sh-button default-value="second-view">Second</sh-button>
      </sh-tabs>

      <sh-view-swapper variant="secondary" receive="demo-swapper">
        <div id="first-view">
          <p>First Tab</p>
        </div>
        <div id="second-view">
          <p>Second Tab</p>
        </div>
      </sh-view-swapper>
    </lib-toolbar>
    <div style="height:80vh;"></div>
  </section>

  <section id="changelog">
    <h2>Changelog</h2>
    <dl>
      <dt>1.0.0</dt>
      <dd>Tabs was created.</dd>
    </dl>
  </section>
</div>

<%- htmlWebpackPlugin.options.docsFoot %>
