  </section>

  <section id="examples">
    <h2>Examples</h2>
    <h3>Default</h3>
    <lib-toolbar>
      <div style="width: 20rem">
        <sh-combo>
          <ul slot="options">
            <li>One thing</li>
            <li>Two thing</li>
            <li>Red thing</li>
            <li>Blue thing</li>
          </ul>
        </sh-combo>
      </div>
    </lib-toolbar>
    <h3>Primary variant</h3>
    <lib-toolbar>
      <div style="width: 20rem">
        <sh-combo variant="primary">
          <ul slot="options">
            <li>One thing</li>
            <li>Two thing</li>
            <li>Red thing</li>
            <li>Blue thing</li>
          </ul>
        </sh-combo>
      </div>
    </lib-toolbar>
  </section>

  <section id="changelog">
    <h2>Changelog</h2>
    <dl>
      <dt>0.1.0</dt>
      <dd>Combo is under development.</dd>
