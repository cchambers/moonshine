<%- htmlWebpackPlugin.options.docsHead %>

<title><%= htmlWebpackPlugin.options.title %></title>

<div class="documentation contain">
  <h1>Combo</h1>
  <section id="usage">
    <h2>Usage</h2>
    <pre>
      <code class="language-html">
          &lt;sh-combo&gt;
            &lt;div slot="options"&gt;
          &lt;/sh-combo&gt;
      </code>
    </pre>

  </section>

