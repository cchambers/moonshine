<%- htmlWebpackPlugin.options.docsHead %>

<title><%= htmlWebpackPlugin.options.title %></title>

<div class="documentation contain">
  <h1>Belk Price</h1>
  <section id="usage">
    <h2>Usage</h2>
    <pre>
      <code class="language-html">
          &lt;belk-price&gt;new component&lt;/belk-price&gt;
      </code>
    </pre>

  </section>

  <section id="reference" class="baseline30">
    <h2>Reference</h2>
    <h3 id="attributes">Attributes</h3>

    <table class="attributes">
      <thead>
        <tr>
          <th>Attribute</th>
          <th>Type</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>price</td>
          <td>Number</td>
          <td>original price value</td>
        </tr>
        <tr>
          <td>sale_price</td>
          <td>Number</td>
          <td>sale price value</td>
        </tr>
          <td>price_range</td>
          <td>Array</td>
          <td>original price value as an array</td>
        </tr>
        <tr>
          <td>sale_price</td>
          <td>Array</td>
          <td>sale price value as an array</td>
        </tr>
        <tr>
          <td>show-percent</td>
          <td>Boolean</td>
          <td>Show the "(% off)" section</td>
        </tr>
        <tr>
          <td>coupon</td>
          <td>Boolean</td>
          <td>Show the "after coupon" section</td>
        </tr>
      </tbody>
    </table>

    <h3 id="slots">Slots</h3>

    <table class="slots">
      <thead>
        <tr>
          <th>Slot</th>
          <th>Description</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>default</td>
          <td>
            Text content.
          </td>
        </tr>
      </tbody>
    </table>

    <h3 id="methods">Public Methods</h3>

