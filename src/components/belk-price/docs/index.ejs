  </section>

  <section id="examples">
    <h2>Examples</h2>
    <h3>Default</h3>
    <lib-toolbar>
      <belk-price variant="add" unique-id="demo" price="60" sale_price="30" show-percent></belk-price>
    </lib-toolbar>
  </section>

  <section id="changelog">
    <h2>Changelog</h2>
